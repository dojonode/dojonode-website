    "predeploy": "pnpm run build",
    "deploy": "gh-pages -d build --dotfiles"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.7",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.38.0",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "svelte": "^4.2.15",
    "svelte-check": "^3.7.0",
    "tailwindcss": "^3.4.3",
    "tw-colors": "^3.3.1",
    "vite": "^5.2.11"
  },
  "type": "module",
  "dependencies": {
    "ethereum-unit-converter": "^0.0.17",
    "pocketbase": "^0.20.3"
